		"mobx": "^6.13.5",
		"mobx-react": "^9.2.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router-dom": "^7.1.3",
		"sass": "^1.83.4",
		"uuid": "^11.0.5",
