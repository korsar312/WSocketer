		"jwt-decode": "^4.0.0",
		"mobx": "^6.8.0",
		"mobx-react": "^9.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.8.1",
		"sass": "^1.58.3",
		"uuid": "^9.0.1",
		"vite-plugin-svgr": "^4.2.0",
